{
  "name": "multicursorex",
  "displayName": "multi-cursor++",
  "description": "",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [

  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "multicursorex.padding.pad",
        "title": "MultiCursor++: Pad Selections/Cursors"
      },
      {
        "command": "multicursorex.padding.originUp",
        "title": "MultiCursor++: Cycle Origin Upwards"
      },
      {
        "command": "multicursorex.padding.originDown",
        "title": "MultiCursor++: Cycle Origin Downwards"
      },
      {
        "command": "multicursorex.padding.originShow",
        "title": "MultiCursor++: Show Origin"
      },
      {
        "command": "multicursorex.padding.lockOriginToMedian",
        "title": "MultiCursor++: Lock Origin to Selection/Cursor Median"
      },
      {
        "command": "multicursorex.scope.on",
        "title": "hello"
      }
      
    ],
    "keybindings":[
      
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
